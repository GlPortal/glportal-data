{
	"video": {
		"width": "auto",
		"height": "auto",
		"fullscreen": true,
		"antialiasing": 1,
		"vsync": true,
		"recursive_portal": 3
	},
	"mouse": {
		"sensitivity": 0.30,
		"hide_portals_by_click": true,
		"cursor_visibility": false
	},
	"sound": {
		"enabled": true
	},
	"logging": {
		"loglevel": "debug"
	},
	"debug": {
		"gl_context_debug": false,
		"profiler": {
			"enable": false
		}
	}
}
